N,M = map(int,input().split())
check = [False]*(N+1)
answer = []

def sol(index):
    if index == M:
        for i in range(M):
            print(answer[i],end=' ')
        print()
        return
    
    for i in range(1,N+1):
        if check[i]:
            continue
        check[i] = True
        answer.append(i)
        sol(index+1)
        answer.pop()
        
        for j in range(i+1,N+1):
            check[j] = False
sol(0)
